{
  "name": "svelte-kit",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "gen": "graphql-codegen --config ./.graphqlrc.yaml --require dotenv/config",
    "generate": "graphql-codegen --config ./codegen.yaml",
    "getSchema": " gq $(grep '^VITE_NHOST_BACKEND_URL' .env | cut -d '=' -f2)/v1/graphql -H 'X-Hasura-Admin-Secret: '$(grep '^VITE_NHOST_ADMIN_SECRET' .env | cut -d '=' -f2)'' --introspect --format json  > src/lib/graphql/schema.json"
  },
  "devDependencies": {
    "@playwright/test": "^1.21.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-netlify": "^1.0.0-next.51",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module",
  "dependencies": {
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-operations": "^2.3.5",
    "@supabase/supabase-js": "^1.35.2",
    "blueprint-css": "next",
    "graphql": "^16.4.0",
    "graphql-codegen-svelte-apollo": "^1.1.0",
    "graphql-request": "^4.2.0",
    "node-sass": "^7.0.1",
    "svelte-forms-lib": "^2.0.1",
    "swiper": "^8.0.7",
    "yup": "^0.32.11"
  }
}
